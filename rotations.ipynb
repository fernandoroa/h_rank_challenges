    "def circularArrayRotation(array, rotations, queries):\n",
    "    for rotation in range(rotations):\n",
    "      array.insert(0, array.pop(-1))\n",
    "    res = list(map(array.__getitem__, queries))\n",
    "    return res"
   ]
