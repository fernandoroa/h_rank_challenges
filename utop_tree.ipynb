    "def utopianTree(n):\n",
    "    value = 0\n",
    "    for cycle in range(0,61):\n",
    "      if cycle % 2 == 0:\n",
    "        value += 1\n",
    "      else:\n",
    "        value *= 2\n",
    "      if cycle == n:\n",
    "        return value"
   ]
